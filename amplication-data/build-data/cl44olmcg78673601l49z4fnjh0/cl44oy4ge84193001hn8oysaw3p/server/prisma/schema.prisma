datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  firstName       String?
  id              String   @id @default(cuid())
  lastName        String?
  location        String?
  mail            String?
  password        String
  roles           String[]
  telephoneNumber String?
  username        String   @unique
}